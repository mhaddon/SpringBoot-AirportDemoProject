docker stop -f $(docker ps -a -q) > /dev/null 2>&1
docker rm -f $(docker ps -a -q) > /dev/null 2>&1
docker rmi -f $(docker images -a -q) > /dev/null 2>&1
docker pull mysql
docker pull openjdk
docker pull nginx:alpine
mvn clean package
mvn install dockerfile:build

cd ./proxy
docker build -t mhaddon/airportdemo_proxy .
cd ../mysqldb
docker build -t mhaddon/airportdemo_mysqldb .


docker push mhaddon/airportdemo_proxy
docker push mhaddon/airportdemo_mysqldb
docker push mhaddon/airportdemo